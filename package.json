		"@sentry/replay": "^7.116.0",
		"@sentry/sveltekit": "^9.9.0",
		"@stripe/stripe-js": "^5.6.0",
		"@ungap/structured-clone": "^1.3.0",
		"@zerodevx/svelte-toast": "^0.9.6",
		"dayjs": "^1.11.13",
