{
	"name": "sveltefrontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:staging": "vite build --mode staging && vite preview",
		"test:android": "vite build --mode staging && npx cap sync && npx cap run android --target R5CXC0A5L2L",
		"test:ios": "vite build --mode staging && npx cap sync && npx cap run ios --target [TARGET]",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint . && prettier --check .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@capacitor/assets": "^3.0.5",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
